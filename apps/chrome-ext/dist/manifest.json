{
  "manifest_version": 2,
  "name": "ccsseraphini chrome extension",
  "version": "1.0.0",
  "browser_action": {
    "default_icon": "icon-16.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/backgroundPage.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "js": [
        "js/content.js"
      ],
      "matches": [
      ],
      "exclude_matches": [],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "notifications",
    "storage",
    "proxy",
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "webNavigation"
  ]
}
